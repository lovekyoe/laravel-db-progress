{
    "name": "noxxie/laravel-db-progress",
    "description": "Progress is a simple odbc progress service provider for Laravel. It provides odbc Connection by extending the Illuminate Database component of the laravel framework.",
    "keywords": [
        "laravel",
        "Progress",
        "Database",
        "PDO",
        "ODBC"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick",
            "email": "patrick@noxxie.nl",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/database": ">=6.0"
    },
    "autoload": {
        "psr-4": {
            "Noxxie\\Database\\Progress\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Noxxie\\Database\\Progress\\ProgressServiceProvider"
            ]
        }
    }
}
